{
  "name": "Interactive Math Explorer",
  "version": "1.0.0",
  "description": "An interactive web application for visualizing complex mathematical concepts including fractals, function graphs, prime number spirals, and Fourier series",
  "author": "Math Visualization Team",
  "type": "Educational Web Application",
  "technologies": [
    "HTML5",
    "CSS3",
    "JavaScript",
    "Canvas API"
  ],
  "features": [
    "Mandelbrot Fractal Visualization",
    "Interactive Function Plotter",
    "Prime Number Spiral Generator",
    "Fourier Series Animation"
  ],
  "modules": {
    "fractal": {
      "description": "Renders Mandelbrot set with zoom and pan",
      "parameters": {
        "iterations": "10-100",
        "zoom": "1-200"
      }
    },
    "graph": {
      "description": "Plots mathematical functions",
      "supportedFunctions": [
        "trigonometric",
        "polynomial",
        "exponential",
        "logarithmic"
      ]
    },
    "prime": {
      "description": "Visualizes prime numbers in spiral pattern",
      "range": "100-5000 primes"
    },
    "fourier": {
      "description": "Animates Fourier series decomposition",
      "harmonics": "1-20"
    }
  },
  "compatibility": {
    "browsers": [
      "Chrome 90+",
      "Firefox 88+",
      "Safari 14+",
      "Edge 90+"
    ],
    "devices": [
      "Desktop",
      "Tablet",
      "Mobile"
    ]
  }
}
